        function resetBtn() {
            console.log("button clicked")
            let itemsToHide = document.querySelectorAll("schedule-list-content")
            itemsToHide.forEach(exercise => exercise.addEventListener("click", (event) => {
                event.preventDefault();
                style.display = "hide"
            }))
            // $(document).ready(function(){
